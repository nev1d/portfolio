{
    "plugins": [
        ["postcss-import", { "path": ["./src"] }],
        "autoprefixer",
        "postcss-calc",
        "postcss-color-function",
        ["postcss-color-mod-function", { "unresolved": "warn" }],
        "postcss-each",
        "postcss-for",
        "postcss-inherit",
        "postcss-mixins",
